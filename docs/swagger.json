    "definitions": {
        "global.Config": {
            "type": "object",
            "properties": {
                "captcha": {
                    "description": "Captcha\n\t@Description:验证码配置",
                    "type": "object",
                    "properties": {
                        "expireTime": {
                            "description": "验证码过期时间(seconds)",
                            "type": "integer"
                        },
                        "sendEmail": {
                            "description": "是否发送邮件验证码",
                            "type": "boolean"
                        }
                    }
                },
                "email": {
                    "description": "Email\n\t@Description:Email配置",
                    "type": "object",
                    "properties": {
                        "from": {
                            "description": "发件人邮箱",
                            "type": "string"
                        },
                        "host": {
                            "description": "SMTP 服务器地址",
                            "type": "string"
                        },
                        "port": {
                            "description": "SMTP 端口(默认 465)",
                            "type": "integer"
                        },
                        "smtpPass": {
                            "description": "SMTP 密钥（开启SMTP时获取的密钥，而非邮箱密码）",
                            "type": "string"
                        },
                        "smtpUser": {
                            "description": "SMTP 用户名(邮箱账号)",
                            "type": "string"
                        }
                    }
                },
                "jwt": {
                    "description": "JWT\n\t@Description:JWT配置",
                    "type": "object",
                    "properties": {
                        "expire": {
                            "description": "JWT过期时间(hours)",
                            "type": "integer"
                        },
                        "issuer": {
                            "description": "JWT签发者",
                            "type": "string"
                        },
                        "secret": {
                            "description": "JWT密钥",
                            "type": "string"
                        }
                    }
                },
                "log": {
                    "description": "Log\n\t@Description:\t日志配置",
                    "type": "object",
                    "properties": {
                        "directory": {
                            "description": "日志目录",
                            "type": "string"
                        },
                        "format": {
                            "description": "日志格式 (json | text)",
                            "type": "string"
                        },
                        "level": {
                            "description": "日志级别 (debug | info | warn | error )",
                            "type": "string"
                        },
                        "prefix": {
                            "description": "日志前缀",
                            "type": "string"
                        }
                    }
                },
                "mysql": {
                    "description": "Mysql\n\t@Description:Mysql数据库配置",
                    "type": "object",
                    "properties": {
                        "config": {
                            "description": "Mysql 高级配置",
                            "type": "string"
                        },
                        "dbname": {
                            "description": "Mysql 数据库名称",
                            "type": "string"
                        },
                        "host": {
                            "description": "Mysql 服务器地址",
                            "type": "string"
                        },
                        "password": {
                            "description": "Mysql 密码",
                            "type": "string"
                        },
                        "port": {
                            "description": "Mysql 服务器端口",
                            "type": "string"
                        },
                        "username": {
                            "description": "Mysql 用户名",
                            "type": "string"
                        }
                    }
                },
                "qiniu": {
                    "description": "Qiniu\n\t@Description:七牛云配置",
                    "type": "object",
                    "properties": {
                        "accessKey": {
                            "description": "七牛云访问密钥",
                            "type": "string"
                        },
                        "bucket": {
                            "description": "空间名称",
                            "type": "string"
                        },
                        "imgPath": {
                            "description": "外链图片访问路径",
                            "type": "string"
                        },
                        "secretKey": {
                            "description": "七牛云私密密钥",
                            "type": "string"
                        },
                        "useCdnDomains": {
                            "description": "是否使用CDN上传加速",
                            "type": "boolean"
                        },
                        "useHTTPS": {
                            "description": "是否使用https",
                            "type": "boolean"
                        },
                        "zone": {
                            "description": "存储区域",
                            "type": "string"
                        }
                    }
                },
                "redis": {
                    "description": "Redis\n\t@Description:Redis数据库配置",
                    "type": "object",
                    "properties": {
                        "addr": {
                            "description": "Redis 服务器地址:端口",
                            "type": "string"
                        },
                        "db": {
                            "description": "Redis 数据库引索",
                            "type": "integer"
                        },
                        "password": {
                            "description": "Redis 密码",
                            "type": "string"
                        }
                    }
                },
                "server": {
                    "description": "Server\n\t@Description:\t服务器配置",
                    "type": "object",
                    "properties": {
                        "dbAutoMigrate": {
                            "description": "数据库表结构是否自动迁移",
                            "type": "boolean"
                        },
                        "dbLogMode": {
                            "description": "数据库日志模式（silent | error | warn | info）",
                            "type": "string"
                        },
                        "dbType": {
                            "description": "数据库类型",
                            "type": "string"
                        },
                        "mode": {
                            "description": "服务器模式(debug | release)",
                            "type": "string"
                        },
                        "port": {
                            "description": "服务器端口号",
                            "type": "string"
                        }
                    }
                },
                "session": {
                    "description": "Session\n\t@Description:Session配置",
                    "type": "object",
                    "properties": {
                        "maxAge": {
                            "description": "session 过期时间（seconds）",
                            "type": "integer"
                        },
                        "name": {
                            "description": "session 名称",
                            "type": "string"
                        },
                        "salt": {
                            "description": "session 盐值",
                            "type": "string"
                        }
                    }
                },
                "sqlite": {
                    "description": "SQLite\n\t@Description:SQLite数据库配置",
                    "type": "object",
                    "properties": {
                        "dsn": {
                            "description": "SQLite 数据源名称（DSN）",
                            "type": "string"
                        }
                    }
                },
                "upload": {
                    "description": "Upload\n\t@Description:文件上传配置",
                    "type": "object",
                    "properties": {
                        "ossType": {
                            "description": "OSS存储类型(local | qiniu)",
                            "type": "string"
                        },
                        "path": {
                            "description": "本地文件访问路径",
                            "type": "string"
                        },
                        "size": {
                            "description": "上传文件大小限制(字节)",
                            "type": "integer"
                        },
                        "storePath": {
                            "description": "本地文件存储路径",
                            "type": "string"
                        }
                    }
                }
            }
        },
        "handle.LoginReq": {
            "type": "object",
            "required": [
                "password",
                "username"
            ],
            "properties": {
                "password": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "handle.LoginVO": {
            "type": "object",
            "properties": {
                "article_like_set": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "avatar": {
                    "description": "头像最大长度1024字符，保存用户的头像",
                    "type": "string"
                },
                "comment_like_set": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "created_at": {
                    "type": "string"
                },
                "email": {
                    "description": "邮箱最大长度30字符，保存用户的电子邮件地址",
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "intro": {
                    "description": "简介最大长度255字符，保存用户的简介",
                    "type": "string"
                },
                "nickname": {
                    "description": "昵称最大长度30字符，保存用户的昵称",
                    "type": "string"
                },
                "token": {
                    "description": "点赞 Set： 用于记录用户点赞过的文章，评论",
                    "type": "string"
                },
                "updated_at": {
                    "type": "string"
                },
                "website": {
                    "description": "网站最大长度255字符，保存用户的网站等链接",
                    "type": "string"
                }
            }
        },
        "handle.RegisterReq": {
            "type": "object",
            "required": [
                "email",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string",
                    "maxLength": 20,
                    "minLength": 4
                }
            }
        },
        "handle.Response-any": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "业务状态码",
                    "type": "integer"
                },
                "data": {
                    "description": "响应数据"
                },
                "message": {
                    "description": "响应消息",
                    "type": "string"
                }
            }
        },
        "handle.Response-handle_LoginVO": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "业务状态码",
                    "type": "integer"
                },
                "data": {
                    "description": "响应数据",
                    "allOf": [
                        {
                            "$ref": "#/definitions/handle.LoginVO"
                        }
                    ]
                },
                "message": {
                    "description": "响应消息",
                    "type": "string"
                }
            }
        },
        "handle.Response-string": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "业务状态码",
                    "type": "integer"
                },
                "data": {
                    "description": "响应数据",
                    "type": "string"
                },
                "message": {
                    "description": "响应消息",
                    "type": "string"
                }
            }
        }
    }
}